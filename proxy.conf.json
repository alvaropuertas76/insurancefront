{
  "/api/*": {
    "target": "http://localhost:8080/",
    "secure": false,
    "pathRewrite": {
      "^/api": "/"
    },
    "changeOrigin": true
  },
  "/api2/*": {
    "target": "http://localhost:8082/",
    "secure": false,
    "pathRewrite": {
      "^/api2": "/"
    },
    "changeOrigin": true
  }
}